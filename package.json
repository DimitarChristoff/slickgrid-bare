{
  "name": "slickgrid-bare",
  "version": "1.0.0",
  "description": "Non-tracking fork of X-SlickGrid with interact.js and jquery 3.4.1 as external. Barebones, don't use if you don't understand.",
  "main": "dist/slick.min.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --optimize-occurence-order --optimize-dedupe --hot --inline --debug --compress --port 8888",
    "build": "rimraf dist && cross-env NODE_ENV=production webpack --progress --",
    "test": "echo \"Error: no test specified\" && exit 1",
    "eslint": "eslint config",
    "eslint:fix": "npm run eslint -- --fix"
  },
  "author": "Michael Leibman",
  "license": "MIT",
  "dependencies": {
    "jquery": "^3.4.1"
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-runtime": "^6.6.1",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^6.0.3",
    "css-loader": "^0.27.3",
    "eslint": "^3.17.1",
    "eslint-config-standard": "^7.0.1",
    "eslint-plugin-babel": "^4.1.0",
    "eslint-plugin-import": "^2.0.8",
    "eslint-plugin-promise": "^3.0.0",
    "eslint-plugin-standard": "^1.3.1",
    "file-loader": "^0.10.1",
    "html-loader": "^0.4.3",
    "image-webpack-loader": "^5.0.0",
    "node-sass": "^4.12.0",
    "rimraf": "^2.5.3",
    "sass-loader": "^7.0.1",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.15.0",
    "webpack-dev-server": "1.16.5",
    "webpack-node-externals": "^1.5.4"
  }
